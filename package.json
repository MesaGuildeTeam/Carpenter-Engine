{
  "name": "@mesaguilde/carpenter-engine",
  "version": "0.1.3",
  "description": "A C++ game engine with a build once, run anywhere solution",
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "start": "npx table setup",
